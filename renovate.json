{
  "extends": ["config:base"],
  "platform": "github",
  "labels": ["docker", "docker-compose"],
  "autodiscover": true,
  "docker": {
    "enabled": true
  },
    "fileMatch": [
    "(^|/)(?:docker-)?compose[^/]*\\.ya?ml$"
  ],
  "ignorePaths": ["**/*.md", "**/*.json", "**/*.yml"],
  "prHourlyLimit": 5
}
